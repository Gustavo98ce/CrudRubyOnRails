<p id="notice"><%= notice %></p>

<h1>Proprietarios</h1>

<table>
  <thead>
    <tr>
      <th>Cpf ou cnpj</th>
      <th>Nome</th>
      <th>Endereco</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proprietarios.each do |proprietario| %>
      <tr>
        <td><%= proprietario.cpf_cnpj %></td>
        <td><%= proprietario.nome %></td>
        <td><%= proprietario.endereco %></td>
        <td><%= link_to 'Mostrar', proprietario %></td>
        <td><%= link_to 'Editar', edit_proprietario_path(proprietario) %></td>
        <td><%= link_to 'Apagar', proprietario, method: :delete, data: { confirm: 'VocÃª tem certeza ?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

